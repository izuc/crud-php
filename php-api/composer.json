        "php": ">=7.3",
        "firebase/php-jwt": "^6.1",
        "illuminate/database": "^10.48",
        "php-di/php-di": "^6.3",
        "php-di/slim-bridge": "^3.4",
        "selective/basepath": "^2.0",
